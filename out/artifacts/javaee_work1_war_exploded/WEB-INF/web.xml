<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>WebBasic</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>testservlet</servlet-name>
        <!--<servlet-class>com.javaee.code.class12.servlet.TestServlet</servlet-class>-->
        <servlet-class>com.code.servlet.StudentHomeworkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testservlet</servlet-name>
        <url-pattern>/test1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addservlet</servlet-name>
        <!--<servlet-class>com.javaee.code.class12.servlet.TestServlet</servlet-class>-->
        <servlet-class>com.code.servlet.AddStudentHomeworkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addservlet</servlet-name>
        <url-pattern>/test2</url-pattern>
    </servlet-mapping>

</web-app>